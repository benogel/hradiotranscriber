{
  "name": "webpack-es6-boilerplate",
  "version": "1.0.1",
  "author": "Joseph Luccisano <joseph.luccisano@gmail.com> (https://jluccisano.github.io)",
  "description": "A delightfully Webpack ES6 Boilerplate",
  "license": "MIT",
  "keywords": [
    "webpack",
    "boilerplate",
    "es6",
    "babel",
    "eslint",
    "jest",
    "sass"
  ],
  "bugs": {
    "url": "https://github.com/jluccisano/webpack-es6-boilerplate/issues",
    "email": "joseph.luccisano@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jluccisano/webpack-es6-boilerplate.git"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --mode=development --env=development --config conf/webpack.dev.js --host 0.0.0.0",
    "dev": "webpack-dev-server --mode=development",
    "build": "webpack --mode=production --env=production --progress --profile --colors",
    "lint": "eslint ./src/**.js",
    "coverage": "jest --coverage --config=conf/jest.config.js",
    "test": "jest --config=conf/jest.config.js",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.1.0",
    "babel-loader": "8.0.5",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "clean-webpack-plugin": "1.0.1",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "2.1.0",
    "eslint": "5.13.0",
    "eslint-loader": "2.1.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.1.0",
    "script-loader": "0.7.2",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "uglify-js": "3.4.9",
    "uglifyjs-webpack-plugin": "2.1.1",
    "webpack": "4.29.3",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1"
  },
  "dependencies": {
    "bitwise": "^2.0.1",
    "butils": "^0.1.0"
  }
}
